bool isBalanced(string b) 
{
    stack<char> s;
    
    for(int i=0;b[i]!='\0';i++){
        if(b[0]==')')
        {
            return false;
        }
        if(b[i]=='(')
        {
           s.push(')');    
        }
        else{
            if(s.size() ==0){
                return false;
            }
            s.pop();
        }
        
    }
    
    if(s.size()==0){
        return true;
    }
    else
     return false;    
}
